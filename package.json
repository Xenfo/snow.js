{
  "name": "@xenfo/snow.js",
  "version": "2.0.0",
  "description": "A Discord.JS framework inspired by Discord-Akairo",
  "main": "./dist/index.js",
  "repository": "https://github.com/Xenfo/snow.js",
  "author": "Samuel Corsi-House <chouse.samuel@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "tsup && is-ci || husky install",
    "commit": "cz",
    "build": "tsup",
    "watch": "tsup --watch",
    "lint": "eslint --fix --ignore-path .gitignore .",
    "format": "prettier --write --ignore-path .prettierignore ."
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/cz-commitlint": "^16.1.0",
    "@swc/core": "^1.2.135",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "commitizen": "^4.2.4",
    "doctoc": "^2.1.0",
    "eslint": "^8.8.0",
    "eslint-config-marine": "^9.3.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1",
    "tslib": "^2.3.1",
    "tsup": "^5.11.12",
    "typescript": "^4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
