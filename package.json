{
  "name": "@xenfo/snow.js",
  "version": "1.2.0",
  "description": "A Discord.JS framework inspired by Discord-Akairo.",
  "repository": "https://github.com/Xenfo/snow.js",
  "author": "Samuel Corsi-House <chouse.samuel@gmail.com>",
  "license": "MIT",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepare": "is-ci || husky install",
    "build": "rm -rf dist/ && tsc",
    "watch": "tsc --watch",
    "dpack": "yarn build && yarn pack",
    "pub": "yarn build && yarn publish --no-git-tag-version",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .js,.ts --fix .",
    "lint:prettier": "prettier --write --ignore-path .gitignore *.json"
  },
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.5.0",
    "discord-api-types": "^0.36.1",
    "discord.js": "^13.8.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "doctoc": "^2.2.0",
    "eslint": "^8.19.0",
    "eslint-config-marine": "^9.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.2.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
