{
  "name": "@xenfo/snow.js",
  "version": "1.0.5",
  "description": "A Discord.JS framework inspired by Discord-Akairo.",
  "repository": "https://github.com/Xenfo/snow.js",
  "author": "Samuel Corsi-House <chouse.samuel@gmail.com>",
  "license": "MIT",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "pub": "yarn build && yarn publish",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx --fix .",
    "lint:prettier": "prettier --write --ignore-path .gitignore *.json"
  },
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@trivago/prettier-plugin-sort-imports": "^2.0.4",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "doctoc": "^2.0.1",
    "eslint": "^7.32.0",
    "eslint-config-marine": "^9.0.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
